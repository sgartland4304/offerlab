{
  "name": "brand-collab-finder",
  "version": "1.0.0",
  "description": "AI-powered brand collaboration finder",
  "type": "module",
  "scripts": {
    "dev": "node server.js",
    "start": "node server.js",
    "serve": "npx serve -l 5500 .",
    "build:css": "npx @tailwindcss/cli -i ./src/input.css -o ./tailwind.css",
    "watch:css": "npx @tailwindcss/cli -i ./src/input.css -o ./tailwind.css --watch",
    "deploy": "wrangler pages deploy . --project-name=collab-finder",
    "deploy:preview": "wrangler pages deploy . --project-name=collab-finder --branch=preview"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "express": "^4.21.0"
  },
  "devDependencies": {
    "@tailwindcss/cli": "^4.1.18",
    "tailwindcss": "^4.1.18",
    "wrangler": "^4.62.0"
  }
}
